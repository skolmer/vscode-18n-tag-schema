{
  "name": "vscode-i18n-tag-schema",
  "displayName": "i18n literals schema generator",
  "description": "JSON Schema for i18n tagged template literals",
  "version": "0.3.0",
  "publisher": "skolmer",
  "author": "Steffen Kolmer <steffen@kolmer.net> (http://github.com/skolmer)",
  "license": "MIT",
  "engines": {
    "vscode": "^0.10.7"
  },
  "icon": "images/vscode-18n-tag-schema-icon.jpg",
  "categories": [
    "Other"
  ],
  "galleryBanner": {
    "color": "#f2e058",
    "theme": "light"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/skolmer/vscode-18n-tag-schema.git"
  },
  "activationEvents": [
    "onCommand:i18nTag.updateSchema"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "commands": [
      {
        "command": "i18nTag.updateSchema",
        "title": "i18nTag: Update translation schema"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "node ./node_modules/vscode/bin/compile",
    "compile": "node ./node_modules/vscode/bin/compile -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "dependencies": {
    "i18n-tag-schema": "^0.5.0"
  },
  "devDependencies": {
    "typescript": "^1.7.5",
    "typings": "^1.2.0",
    "vscode": "^0.11.0"
  }
}
